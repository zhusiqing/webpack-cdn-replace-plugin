{
  "name": "webpack-cdn-replace-plugin",
  "version": "0.1.0",
  "description": "webpack upload and cdn address replace plugin",
  "main": "lib/index.js",
  "scripts": {
    "test": "webpack --config example/webpack.config.js",
    "test:tsc": "tsc && webpack --config example/webpack.config.js",
    "lint": "eslint --cache \"src/**/*.{js,ts}\" --fix",
    "prepare": "husky install"
  },
  "keywords": [
    "webpack plugin",
    "cdn plugin",
    "webpack upload plugin",
    "webpack cdn plugin",
    "webpack cdn replace plugin"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "pnpm run lint",
      "git add"
    ]
  },
  "engines": {
    "node": ">= 12.0.0",
    "pnpm": ">=7"
  },
  "author": "siqing",
  "dependencies": {
    "consola": "^2.15.3",
    "fs-extra": "^10.1.0",
    "ora": "^5.4.1"
  },
  "devDependencies": {
    "@types/consola": "^2.2.5",
    "@types/fs-extra": "^9.0.13",
    "@types/md5": "^2.3.2",
    "commitizen": "^4.2.4",
    "css-loader": "^6.7.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-define-config": "^1.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "file-loader": "^6.2.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "md5": "^2.3.0",
    "mini-css-extract-plugin": "^2.6.1",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  },
  "license": "MIT"
}
